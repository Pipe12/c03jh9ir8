<h1>Actores</h1>

<table>
	<tr>
		<th>Foto</th>
		<th>Nombre</th>	
		<th>Bio</th>
		<th>Fecha de nacimiento</th>
		<th>Lugar de nacimiento</th>
	</tr>
	<% @actors.each do |actor| %>
		<tr>
			<td><%= actor.image_url %></td>
			<td><%= actor.name %></td>
			<td><%= actor.bio %></td>
			<td><%= actor.birth_date %></td>
			<td><%= actor.birth_place %></td>
		</tr>
	<% end %>
</table>
